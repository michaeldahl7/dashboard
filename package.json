{
   "name": "kitchn",
   "private": true,
   "type": "module",
   "scripts": {
      "dev": "vinxi dev",
      "build": "vinxi build",
      "start": "vinxi start",
      "format-and-lint": "biome check .",
      "format-and-lint:fix": "biome check . --write",
      "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
      "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
      "db:push": "drizzle-kit push --config=drizzle.config.ts",
      "db:seed": "bun run lib/server/db/seed.ts",
      "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
      "ui": "bunx --bun shadcn@latest",
      "auth": "bunx --bun @better-auth/cli@latest",
      "auth:generate": "bunx @better-auth/cli@latest generate --y --output ./lib/server/schema/auth.schema.ts && bunx biome format --write ./lib/server/schema/auth.schema.ts"
   },
